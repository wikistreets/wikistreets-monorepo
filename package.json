{
  "name": "wikistreets",
  "version": "1.0.0",
  "private": true,
  "description": "Wikistreets is a simple yet powerful online map maker. Snap and map photos and notes while at home or on the road. Share your map and invite others to collaborate. Fork (i.e. copy) any map.",
  "main": "index.js",
  "scripts": {
    "start": "cross-env NODE_ENV=production node index",
    "start-dev": "cross-env NODE_ENV=development nodemon index",
    "test": "cross-env NODE_ENV=test mocha --timeout 5000 __tests__/**/*.test.js",
    "report": "cross-env NODE_ENV=test nyc --reporter=lcov mocha --timeout 5000 __tests__/**/*.test.js",
    "text-report": "cross-env NODE_ENV=test nyc --reporter=text-summary mocha --timeout 5000 __tests__/**/*.test.js",
    "nyc": "cross-env NODE_ENV=test nyc mocha --timeout 5000 __tests__/**/*.test.js",
    "build-browser-js": "webpack --config webpack.config.js"
  },
  "keywords": [],
  "author": "wikistreets.io",
  "license": "ISC",
  "dependencies": {
    "@turf/turf": "^5.1.6",
    "bcryptjs": "^2.4.3",
    "bl": ">=2.2.1",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-validator": "^6.6.1",
    "generate-password": "^1.5.1",
    "gray-matter": "^4.0.2",
    "jpeg-autorotate": "^6.0.0",
    "jsonwebtoken": "^8.5.1",
    "leaflet-editable": "^1.2.0",
    "lodash": "^4.17.20",
    "mongoose": "^5.9.1",
    "morgan": "^1.9.1",
    "multer": "^1.4.2",
    "nodemailer": "^6.4.11",
    "passport": "^0.4.1",
    "passport-jwt": "^4.0.0",
    "passport-local": "^1.0.0",
    "prerender-node": "^3.2.5",
    "serve-static": "^1.14.1",
    "sharp": "^0.25.2"
  },
  "devDependencies": {
    "bootstrap": "^4.5.2",
    "chai": "^4.2.0",
    "chai-http": "^4.3.0",
    "cross-env": "^7.0.2",
    "faker": "^4.1.0",
    "jquery": ">=3.5.0",
    "leaflet": "~1.3.1",
    "leaflet-extra-markers": "^1.2.1",
    "leaflet.markercluster": "^1.4.1",
    "marked": "^2.0.0",
    "mocha": "^7.1.1",
    "nodemon": "^2.0.2",
    "nyc": "^15.0.0",
    "popper.js": "^1.16.1",
    "rewire": "^5.0.0",
    "sinon": "^9.0.1",
    "sinon-chai": "^3.5.0",
    "webpack": "^4.44.2",
    "webpack-cli": "^3.3.12"
  },
  "nyc": {
    "exclude": [
      "__tests__",
      "node_modules"
    ],
    "cache": false,
    "report-dir": "./__tests__/report"
  }
}
